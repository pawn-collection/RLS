public UnSurfCar(carid)
{
    for(new i = 0; i<GetMaxPlayers(); i++)
	{
	    if(GetPlayerSurfingVehicleID(i) == carid)
	    {
			new Float:p[3]; GetPlayerPos(i,p[0],p[1],p[2]);
			GetXYInBackOfPlayer(i,p[0],p[1],2.0);
			SetPlayerPos(i,p[0],p[1],p[2]);
		}
	}
	return ;
}
