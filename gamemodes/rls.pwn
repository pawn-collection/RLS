public OnPlayerAmmoChange(playerid, newammo, oldammo)
{
    if(newammo < oldammo)
    {
        Shot[playerid]++;
        switch(GetPlayerWeapon(playerid))
        {
            case 24: if(Shot[playerid] >= 25 && PlayerInfo[playerid][pDesertEagle] < 100) PlayerInfo[playerid][pDesertEagle]++, Shot[playerid] = 0, SaveAccounts();
            case 23: if(Shot[playerid] >= 25 && PlayerInfo[playerid][pSDPistol] < 100) PlayerInfo[playerid][pSDPistol]++, Shot[playerid] = 0, SaveAccounts();
            case 25: if(Shot[playerid] >= 50 && PlayerInfo[playerid][pShotGun] < 100) PlayerInfo[playerid][pShotGun]++, Shot[playerid] = 0, SaveAccounts();
            case 29: if(Shot[playerid] >= 50 && PlayerInfo[playerid][pMP5] < 100) PlayerInfo[playerid][pMP5]++, Shot[playerid] = 0, SaveAccounts();
            case 30: if(Shot[playerid] >= 50 && PlayerInfo[playerid][pAK47] < 100) PlayerInfo[playerid][pAK47]++, Shot[playerid] = 0, SaveAccounts();
            case 31: if(Shot[playerid] >= 50 && PlayerInfo[playerid][pM4] < 100) PlayerInfo[playerid][pM4]++, Shot[playerid] = 0, SaveAccounts();
        }
    }
    return true;
}

