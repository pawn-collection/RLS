public CheckStatus(playerid)
{
    new carid = GetPlayerVehicleID(playerid);
	new Float:hp;
	new v = GetPlayerVehicleID(playerid);
	GetVehicleHealth(v, hp);
    for(new i=0; i<GetMaxPlayers(); i++)
	{
		if(IsPlayerConnected(i) && gPlayerLogged[i] == 1)
		{
			if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
			{
	    			if(hp < 450)
					{
						zavodis[carid] = 0;
						GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
	 					SetVehicleParamsEx(GetPlayerVehicleID(playerid),0,0,alarm,doors,bonnet,boot,objective);
//						SendClientMessage(playerid, COLOR_LIGHTRED, "{ffffff}Машина разбита. Двигатель не заводится. Вы можете вызвать механика {FF6347}(/service mechanic)");
					}
			}
		}
	}
}

