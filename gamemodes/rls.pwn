				if(OnZONE[i][x] > 0 && IsInAllowedF(x)) asd ++;
			}
			if(asd == 1)
			{
				for(new x =0;x<20;x++)
				{
					if(OnZONE[i][x] > 0)
					{
						new zx,zl;
						zx = x;
						zl = i;
						if(GZInfo[i][gFrakVlad] == x) {format(string,sizeof(string),"Банда %s отстояла свою территорию",GetGangName(zx)); SendGangMessage(i,COLOR_GREEN,string);}
						else
						{format(string,sizeof(string),"Банда %s выйгрывает войну за территорию",GetGangName(zx)); SendGangMessage(i,COLOR_GREEN,string);}
						ZoneOnBattle[zl] =0;
						GZInfo[zl][gNapad] = 0;
						GZInfo[zl][gFrakVlad] = zx;
						GangZoneStopFlashForAll(GZInfo[zl][gID]);
						GangZoneHideForAll(GZInfo[zl][gID]);
						GangZoneShowForAll(GZInfo[zl][gID],GetGangZoneColor(zl));
						SaveGZ();
						break;
					}
				}
			}
		}
	}
	for(new i =0;i<120;i++)
	{
		for(new z = 0;z<20;z++)
		{
			OnZONE[i][z] =0;
		}
	}
}
